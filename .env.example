# ── Enigma-Reason Environment Variables ──────────────────────────────────────
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# App
ENIGMA_APP_NAME=enigma-reason
ENIGMA_DEBUG=false
ENIGMA_LOG_LEVEL=INFO

# CORS (JSON array of allowed origins)
ENIGMA_CORS_ORIGINS=["http://localhost:3000"]

# LLM / Agent Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Database (if needed later)
# DATABASE_URL=postgresql+asyncpg://user:pass@localhost:5432/enigma
